class Count{
	static int a;
	int b;
	void increment() {
		int c;
		a++;
		b++;
		c++;
		System.out.println("static 변수 a:"+a);
		System.out.println("increment 변수 b:"+b);
		System.out.println("지역 변수 c:"+c);
		System.out.println();
	}
}

public class Ex05_06_변수 {
	public static void main(String[] args) {
	}
}