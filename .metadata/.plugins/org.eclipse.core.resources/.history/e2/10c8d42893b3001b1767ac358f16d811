public class Ex03_07_for {
	public static void main(String[] args) {
		for(int i=0;i<10;i++) {
			System.out.println("java"+i);
		}
		
		for(int i=10;i<=14;i++) {
			System.out.println("apple");
			System.out.println("banana");
		}
		
		int sum=0;
		
		for(int i=1; i<=100;i++) {
			if(i%5==0) {
				sum+=i;
//				System.out.println(sum);
			}
		}
		System.out.println("5의배수 합:"+sum);
		
	}
}
