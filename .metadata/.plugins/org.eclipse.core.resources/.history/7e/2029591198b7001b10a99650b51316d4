class Member{
	private String id;
	private String passwd;
	void setId(String id) {
		this.id = id;
	}
	void setPasswd(String passwd) {
		this.passwd = passwd;
	}
	
	String getId() {
		return id;
	}
	String getPasswd() {
		return passwd;
	}
}
public class Ex05_14_setter_getter {
	public static void main(String[] args) {
		Member m = new Member();
		m.setId("아이디");
		m.setPasswd("비밀번호");
		System.out.println(m.getId());
		System.out.println(m.getPasswd());
	}
}