	public static void main(String[] args) {
		Stu s1 = new Stu();
		Stu s2 = new Stu("¼­Çö");
		Stu s3 = new Stu("¼ö¿µ",33);
		s1.display();
		s2.display();
		s3.display();
		
		System.out.println();
		int a=10,b=20,c=30;
		
		int[] arr1 = new int[3];
		int[] arr2 = {a,b,c};
		int[] arr3 = new int[3] {10,20,30};
	}
}