class Stu{
	private String name;
	private int kor,eng;
	
	Stu(){
		this.name = "Á¦¹Î";
		this.kor=25;
		this.eng=95;
	}
	Stu(String s){
		this.name = s;
		this.kor=45;
		this.eng=63;
	}
	Stu(String s, int i){
		this.name = s;
		this.kor = i;
		this.eng=60;
	}
	void display() {
		System.out.println(name+","+kor+","+eng);
	}
}

