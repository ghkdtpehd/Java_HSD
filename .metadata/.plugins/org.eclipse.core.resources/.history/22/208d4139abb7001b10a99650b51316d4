class Book{
	private String title;
	private int price;
	
	void setTitle(String title) {
		this.title = title;
	}
	void setPrice(int price) {
		this.price = price;
	}
	
	String getTitle() {
		return title; 
	}
	int getPrice() {
		return price;
	}
}
public class Ex05_17_황세동 {
	public static void main(String[] args) {
		//book 클레스 객체 3개를 배열로 만들기
		Book b1,b2,b3;
		Book[] arrBk = new Book[3];
		
